{
  "name": "@flatsby/api-convex",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./convex/*": "./convex/*"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "convex dev",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@ai-sdk/google": "^1.0.0",
    "@ai-sdk/openai": "^1.0.0",
    "@better-auth/expo": "^1.2.0",
    "@convex-dev/agent": "^0.2.0",
    "@convex-dev/better-auth": "^0.3.0",
    "@flatsby/validators": "workspace:*",
    "ai": "^4.0.0",
    "autumn-js": "catalog:",
    "better-auth": "catalog:",
    "convex": "^1.22.0",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@flatsby/eslint-config": "workspace:*",
    "@flatsby/prettier-config": "workspace:*",
    "@flatsby/tsconfig": "workspace:*",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@flatsby/prettier-config"
}
