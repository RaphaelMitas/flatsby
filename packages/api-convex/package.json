{
  "name": "@flatsby/api-convex",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./convex/*": "./convex/*"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "convex dev",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@flatsby/validators": "workspace:*",
    "convex": "^1.22.0",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@flatsby/eslint-config": "workspace:*",
    "@flatsby/prettier-config": "workspace:*",
    "@flatsby/tsconfig": "workspace:*",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@flatsby/prettier-config"
}
