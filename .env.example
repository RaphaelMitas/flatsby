# Environment variables example
# Copy this file to .env and fill in the values as needed

# Database
# Required by packages/db and apps/nextjs
DATABASE_URL=

# Next.js server
# Optional: port used in development fallback logic
PORT=3000

# Auth (packages/auth)
BETTER_AUTH_SECRET=

# OAuth providers
# Google
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=


# Apple Sign In
APPLE_KEY_ID=
APPLE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\n...\n-----END PRIVATE KEY-----"
APPLE_TEAM_ID=
APPLE_BUNDLE_ID=
APPLE_SERVICE_ID=

# AI Keys
GOOGLE_GENERATIVE_AI_API_KEY=

# Vercel (optional, used to construct production URLs)
# VERCEL_URL=
# VERCEL_PROJECT_PRODUCTION_URL=