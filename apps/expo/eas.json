{
  "cli": {
    "version": ">= 4.1.2",
    "appVersionSource": "remote",
    "requireCommit": true
  },
  "build": {
    "base": {
      "node": "22.12.0",
      "pnpm": "9.15.4",
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "development": {
      "extends": "base",
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "extends": "base",
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "production": {
      "extends": "base",
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {},
    "internal": {
      "android": {
        "track": "internal",
        "releaseStatus": "draft"
      }
    }
  }
}
